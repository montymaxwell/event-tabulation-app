{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@lib/*": ["./lib"],
      "@components/*": ["./components/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
